<h1>Ironhack Cinema</h1>
<h2>Click on the movie to check the showtimes!</h2>
{{#each results}}
<div class="movieDiv">
    <div class="interiorDiv">
        <img src="{{this.image}}" alt="movie-img">
        <p>{{this.title}}</p>
        <a href="/movie/{{this._id}}">See More</a>
    </div>
</div>
{{/each}}